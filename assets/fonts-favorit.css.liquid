    src: url('{{ "FavoritPro-Book.eot?#iefix" | asset_url }}') format('embedded-opentype'), url('{{ "FavoritPro-Book.woff2" | asset_url }}') format('woff2'), url('{{ "FavoritPro-Book.woff" | asset_url }}') format('woff');

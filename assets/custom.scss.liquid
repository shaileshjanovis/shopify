%section {
    position: relative;
    width: 100%;
    min-height: 1px;
    padding-right: 10px;
    padding-left: 10px;
}
#collection-page-custom {
    width: 90%;
    margin: 50px auto;
    padding-right: 20px;
    padding-left: 20px;
    a{
        display: block;
        position: relative;
        overflow: hidden;
        margin-top: 20px;
    }
    .main-box{
        display: flex;
        flex-wrap: wrap;
        margin-right: -10px;
        margin-left: -10px;
        .section-box1{
            flex: 0 0 33.33%;
            max-width: 33.33%;
            @extend %section;
        }
            .content-box{
                position: relative;
                .img-box{
                    .image-fit{
                        width: 100%;
                        height: auto;
                        transition: all 0.3s linear;
                        border: 1px solid #FFF;
                        &:hover{
                            opacity: 0;
                        }
                    }
                    &:hover{
                        border:1px solid #000;
                    }
                }
                .title-box{
                    position: absolute;
                    top: 0px;
                    left: 0px;
                    p{
                      font-weight:300;
                        color: #2e2a25;
                        font-size: 32px;
                        cursor: pointer;
                        padding: 15px 20px;
                    }
                }
            }
        
        .section-box2{
            flex: 0 0 66.33%;
            max-width: 66.33%;
            @extend %section;
            .main-box{
                display: flex;
                flex-wrap: wrap;
                margin-right: -10px;
                margin-left: -10px;
                .section-box2-child{
                    flex: 0 0 100%;
                    max-width: 100%;
                    @extend %section;
                    .section-top-child{
                        flex: 0 0 33.33%;
                        max-width: 33.33%;
                        @extend %section;
                    }
                    .section-middle-child{
                        flex: 0 0 100%;
                        max-width: 100%;
                        @extend %section;
                    }
                    .section-bottom-child{
                        flex: 0 0 50%;
                        max-width: 50%;
                        @extend %section;
                    }
                }
            }
        }
    }
}

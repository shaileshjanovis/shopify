#shopify-section-menu-bar{
  height: 40px;
  padding: 20px 0px;
  .bm-nav{
    .my-circle{
      border-right: 1px solid #000;
      padding-right: 10px;
      a{
        font-size: 14px;
        color: #000;
        text-decoration: none;
      }
    }
    .wishlist{
      img{
        width: 25px;
        height: 25px;
        object-fit: contain;
      }
    }
  }
}
.submenu-wrap{
  position: absolute;
  left: 0;
  width: 100%;
  top: 60px;
  display: none;
  ul{padding:0;li{margin:0}}
  .submenu-content{
    display: flex;
    background: #fff;
    padding: 40px 60px;
    .submenu-left{
      width: 20%;
      .breakline{
        height: 1px;
        background: #ddd;
        margin: 21px 0;
        width: 60%;
      }
      .shopallBtn{
        a{
          max-width: 130px;
          margin: 0 auto;
          border: 1px solid #000;
          color: #000;
          display: inline-block;
          padding: 10px 25px;
          text-decoration: none;
          margin-bottom: 30px;
        }
      }
      ul{
        li{
          padding: 10px 0 !important;
          margin: 0 !important;
          &.active{
            a{
              color: blue;
            }
          }
          a{
            text-decoration: none;
            color: #000;

          }
        }
      }
    }
    .submenu-right{
      width: 80%;
      .menus-wrap{
        .menus-content{
          display: flex;
          ul{
            width: 33.3%;
            .heading{
              margin-bottom: 10px;
            }
            li{
              margin: 0 !important;
              padding: 0 !important;
              height: fit-content !important;
              a{
                text-decoration: none;
                color: #000;
                display: inline-block;
                width: 100%;
                padding: 6px 0;
                font-weight: normal;
              }
            }
          }
        }
      }
    }
  }
}

.main-nav__desktop li:hover > .submenu-wrap{
  display: block
}

.border-0{
  border: 0 !important;
}

#preview-bar-iframe{
  display: none;
}
.homeBlock-wrap{
  .homeBlock-container{
    .homeBlock-content{
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;

      .block-single{
        width: 49.5%;
        margin-bottom: 10px;

        .block-content{
          position: relative;
          .image-div{
            height: 350px;
            img{
              width: 100%;
              height: 100%;
              object-fit: cover;
            }
          }
          .block-text-wrap{
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: #fff;
            border: 1px solid #000;
            visibility: hidden;
            opacity: 0;
            transition: visibility 0s, opacity 0.5s linear;
            .block-text-content{
              height: 100%;
              display: flex;
              align-items: center;
              justify-content: center;
              .text-content{
                padding: 20px;
                text-align: center;
                .block-heading{
                }
                .block-para{
                  padding: 15px 0;
                }
                .block-btn{
                  a{
                    text-transform: capitalize;
                    color: #000;
                    text-decoration: none;
                    position: relative;
                    &::before{
                      content: '';
                      position: absolute;
                      width: 54px;
                      height: 2px;
                      background: #ddd;
                      z-index: 9;
                      bottom: -9px;
                      left: 50%;
                      transform: translateX(-50%);
                    }
                  }
                }
              }
            }
          }
        }
      }
      .block-single:hover{
        .block-text-wrap{
          visibility: visible;
          opacity: 1;
        }
      }
    }
  }
}

